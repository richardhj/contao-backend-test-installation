{
  "name": "contao/managed-edition",
  "type": "project",
  "description": "Contao test installation",
  "require": {
    "php": "^7.4",
    "contao/contao": "dev-tng-backend as 4.12.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/richardhj/contao"
    }
  ],
  "extra": {
    "contao-component-dir": "assets",
    "symfony": {
      "require": "^4.4"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "contao-setup --ansi"
    ],
    "post-update-cmd": [
      "contao-setup --ansi"
    ]
  }
}
